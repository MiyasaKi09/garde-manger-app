{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Export DB schema (session pooler)",
      "type": "shell",
      "command": "PGCONNECT_TIMEOUT=15 pg_dump --schema-only --no-owner --no-privileges --format=p -n public -f schema.sql \"$DATABASE_URL_SESSION\"",
      "problemMatcher": [],
      "options": { "env": { "DATABASE_URL_SESSION": "${env:DATABASE_URL_SESSION}" } }
      
    }
  ]
}
